{
  "name": "bmatovu/laravel-js-routes",
  "description": "Laravel Javascript routes.",
  "homepage": "https://github.com/mtvbrianking/laravel-js-routes",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "lumen",
    "Javascript",
    "routes"
  ],
  "authors": [
    {
      "name": "Brian Matovu",
      "email": "mtvbrianking@gmail.com",
      "homepage": "http://bmatovu.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "illuminate/console": "^7.0",
    "illuminate/routing": "^7.0",
    "illuminate/support": "^7.0",
    "illuminate/filesystem": "^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Bmatovu\\JsRoutes\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bmatovu\\JsRoutes\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "doc": "php sami.phar update .sami.dist",
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bmatovu\\JsRoutes\\JsRoutesServiceProvider"
      ]
    }
  }
}
